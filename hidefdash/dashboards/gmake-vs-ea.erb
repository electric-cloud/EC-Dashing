<link rel="stylesheet" href="/assets/ec.css">
<div class="gridster">
  <ul>
    <li data-row="1" data-col="4" data-sizex="6" data-sizey="1">
        <div data-id="build_elapsed" data-title="Elapsed Time" data-text="Started at" data-view="Elapsed" class="elapsed"></div>
    </li>
    <li data-row="2" data-col="1" data-sizex="6" data-sizey="1">
      <div data-id="gmake" data-title="GNU make" data-text="" data-view="Text" class="gmake-bg"></div>
    </li>
    <li data-row="2" data-col="1" data-sizex="6" data-sizey="1">
      <div data-id="ea" data-title="ElectricAccelerator" data-text="" data-view="Text" class="ea-bg"></div>
    </li>
    <li data-row="3" data-col="1" data-sizex="6" data-sizey="1">
      <div data-id="gmake_progress" data-title="" data-view="ProgressBars" class="gmake-bg"></div>
    </li>
    <li data-row="3" data-col="1" data-sizex="6" data-sizey="1">
      <div data-id="ea_progress" data-title="" data-view="ProgressBars" class="ea-bg"></div>
    </li>
    <li data-row="3" data-col="1" data-sizex="3" data-sizey="2">
      <div data-id="gmake_build_count" data-title="Completed Builds" data-view="Number" class="gmake-build"></div>
    </li>
    <li data-row="3" data-col="1" data-sizex="3" data-sizey="2">
      <div data-id="gmake_build_rate" data-view="Meter" data-title="Avg. Builds / Minute" data-min="0" data-max="32" class="gmake-build"></div>
    </li>
    <li data-row="3" data-col="1" data-sizex="3" data-sizey="2">
      <div data-id="ea_build_count" data-title="Completed Builds" data-view="Number" class="ea-build"></div>
    </li>
    <li data-row="3" data-col="1" data-sizex="3" data-sizey="2">
      <div data-id="ea_build_rate" data-view="Meter" data-title="Avg. Builds / Minute" data-min="0" data-max="20" class="ea-build"></div>
    </li>
    <li data-row="3" data-col="1" data-sizex="6" data-sizey="1">
      <div data-id="gmake_last_builds" data-title="Last 10 build times (secs)" data-color="#000" data-max="50" data-view="Graph" data-graphtype="bar" class="gmake-build"></div>
    </li>
    <li data-row="3" data-col="1" data-sizex="6" data-sizey="1">
      <div data-id="ea_last_builds" data-title="Last 10 build times (secs)" data-color="#000" data-max="50" data-view="Graph" data-graphtype="bar" class="ea-build"></div>
    </li>
  </ul>
</div>
